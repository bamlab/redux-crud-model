{
  "extends": [
    "prettier",
    "prettier/flowtype",
  ],
  "plugins": [
    "flowtype",
    "prettier"
  ],
  "globals": {
    "__DEV__": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true
    }
  },
  "parser": "babel-eslint",
  "rules": {
    "no-use-before-define": 0,
    "no-unused-vars": "error",
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 100
      }
    ]
  }
}
